{
  "name": "vue3-bot-ui",
  "version": "0.1.0",
  "description": "Chatbot UI using Vue.js 3",
  "author": {
    "name": "SonNH",
    "email": "sonnh@ownego.com"
  },
  "main": "dist/vue-bot-ui.common.js",
  "files": [
    "dist/*",
    "src/*",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-bundle": "vite build --lib",
    "preview": "vite preview",
    "release": "standard-version && git push --follow-tags && npm publish",
    "release:next": "standard-version --prerelease next && git push --follow-tags origin dev && npm publish --tag next",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "keywords": [
    "chatbot",
    "bot ui",
    "vue-bot-ui"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-standard": "^9.0.1",
    "bootstrap": "^5.3.6",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "sass": "^1.89.0",
    "standard-version": "^9.5.0",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.14"
  },
  "dependencies": {
    "@npmcli/package-json": "^3.0.0",
    "mitt": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuzSer/vue-bot-ui.git"
  },
  "bugs": {
    "url": "https://github.com/JuzSer/vue-bot-ui/issues"
  },
  "homepage": "https://github.com/JuzSer/vue-bot-ui#readme",
  "license": "MIT"
}
